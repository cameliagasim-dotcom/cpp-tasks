#include <iostream>
using namespace std;
int main() {
  int a,b,c;
  cout<<"birinci ededi daxil edin:";
  cin>>a;
  cout<<"ikinci ededi daxil edin:";
  cin>>b;
  cout<<"ucuncu eddi daxil edin:";
  cin>>c;
  if(a>b){
      if(a>c){
          cout<<a;
      }
  }
  else if(b>a){
      if(b>c){
          cout<<b;
      }
  }
  else if(c>a){
      if(c>b){
          cout<<c;
      }
  }
   
    return 0;
}
