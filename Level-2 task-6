#include <iostream>
using namespace std;
int main() {
 int a;
 cout<<"ayin nomresini daxil edin:";
 cin>>a;
 if(a==12 or a==1 or a==2){
     cout<<"Qis feslidir";
 }
 else if(a==3 or a==4 or a==5){
     cout<<"Yaz feslidir";
 }
 else if(a==6 or a==7 or a==8){
     cout<<"Yay feslidir";
 }
 else if(a==9 or a==10 or a==11){
     cout<<"Payiz feslidir";
 }
 else{
     cout<<"ayin nomresi duzgun daxil edilmeyib";
 }
    return 0;
}
