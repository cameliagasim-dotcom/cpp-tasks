#include <iostream>
using namespace std;
int func(int x){
    int s=0;
   for(int i=1;i<=x;i++){
       int a;
       cout<<"ededi daxil edin:";
       cin>>a;
       s+=a;
        
    }  
    
    cout<<"cem:"<<s;
    return s;
}
int main(){
    int N;
    cout<<"sayi daxil edin:";
    cin>>N;
    func(N);
    return 0;
}
    
