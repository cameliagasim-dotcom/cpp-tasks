#include <iostream>
using namespace std;
void func(int N){
    for(int i=2;i<N;i++){
        int n=0;
        for(int j=1;j<=i;j++){
            if(i%j==0){
                n++;
            }
        }
        if(n==2){
            cout<<i<<" ";
        }
    }
    
}
int main(){
    int x;
    cout<<"ededi daxil edin:";
    cin>>x;
    func(x);
}    
    
